<computer_extension_attribute>
    <name>BonjourBrowserVersion</name>
    <description />
    <data_type>String</data_type>
    <input_type>
        <type>script</type>
        <platform>Mac</platform>
        <script>#!/bin/bash

BonjourBrowserVersion=$(defaults read /Applications/BonjourBrowser.app/Contents/Info.plist CFBundleShortVersionString)

echo "&lt;result&gt;$BonjourBrowserVersion&lt;/result&gt;"

exit 0</script>
    </input_type>
    <inventory_display>Extension Attributes</inventory_display>
    <recon_display>Extension Attributes</recon_display>
</computer_extension_attribute>